.add-btn{
  position: fixed;
  right: 24rpx;
  bottom: 200rpx;
  width: 88rpx;
  height: 88rpx;
  border-radius: 100%;
  background-color: #999;
}
.add-btn::before{
  content: '';
  display: block;
  position: absolute;
  width: 68rpx;
  height: 6rpx;
  top: 40rpx;
  left: 10rpx;
  background-color: #fff;
}
.add-btn::after{
  content: '';
  display: block;
  position: absolute;
  width: 6rpx;
  height: 68rpx;
  top: 10rpx;
  left: 40rpx;
  background-color: #fff;
}

